{
  "name": "@rs-components/ng-motion",
  "version": "0.0.8",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./motion-one.directive": {
      "types": "./src/app/directives/motion-one.directive.d.ts",
      "default": "./src/app/directives/motion-one.directive.js"
    },
    "./motion-if.directive": {
      "types": "./src/app/directives/motion-if.directive.d.ts",
      "default": "./src/app/directives/motion-if.directive.js"
    },
    "./motion.directive": {
      "types": "./src/directives/ngx-motion.directive.d.ts",
      "default": "./src/directives/ngx-motion.directive.js"
    }
  },
  "files": [
    "*.ts",
    "*.js",
    "*.d.ts",
    "src/**/*.ts",
    "src/**/*.js",
    "src/**/*.d.ts",
    "public-api.ts",
    "public-api.js",
    "public-api.d.ts"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rs-components/ng-motion-demo.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:ng-motion-poc": "node dist/ng-motion-poc/server/server.mjs"
  },
  "dependencies": {
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/platform-server": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@angular/ssr": "^19.2.5",
    "express": "^4.18.2",
    "rxjs": "~7.8.0",
    "three": "^0.164.1",
    "tslib": "^2.3.0",
    "tweakpane": "^4.0.5",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.5",
    "@angular/cli": "^19.2.5",
    "@angular/compiler-cli": "^19.2.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "@types/three": "^0.155.0",
    "@types/webgl2": "0.0.7",
    "angular-cli-ghpages": "^2.0.3",
    "autoprefixer": "^10.4.21",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "motion": "^12.4.10",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.2"
  }
}
